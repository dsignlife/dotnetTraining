@model IEnumerable<ASPNETPT.Models.BtCprop>
@{
ViewBag.Title = "Home Page";
}

<div class="row">
  
    <div class ="col-md-6">
        
        <h3>Coin stats</h3>
        <p>BTC</p>

        <form>
            <div class="form-group">
                <label>INPUT1</label>
                <input class="form-control"/>
            </div>
            <div class="form-group">
                <label>INPUT2</label>
                <input class="form-control"/>
            </div>

            <div>
                <input type="submit" value="Add" class="btn btn-success">
            </div>

        </form>
        

        
        <div >
            <p><br />CandleChart</p>
            <div id="candle" style="width: 600px; height: 100%;" class="center-block">
                <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
                <script src="~/js/candlestick.js"></script>
            </div>
        </div>
        
        

    </div>
    
    <div class="col-md-6">
        <h2>BTC Datas</h2>
        

        <ul>        
            @foreach (var item in Model)
        {
            <li>@item.Name : @item.Rate : @item.Date : @item.Time</li>
        }
        </ul>

    </div>

</div>


